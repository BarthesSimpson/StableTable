{
  "name": "stabletable",
  "version": "1.0.0",
  "description": "A datatable for ReactJS",
  "main": "index.js",
  "keywords": [
    "reactjs",
    "datatable"
  ],
  "author": "David Frenk",
  "license": "MIT",
  "scripts": {
    "analyze": "export NODE_ENV=production && export ANALYZE_BUNDLE=true && webpack",
    "build": "export NODE_ENV=production && webpack",
    "start": "export NODE_ENV=development && webpack-dev-server --hot",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "globals": {
      "window": {}
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "^@data(.*)$": "<rootDir>/src/table/data/$1",
      "^@examples(.*)$": "<rootDir>/src/examples/$1",
      "^@presentation(.*)$": "<rootDir>/src/table/presentation/$1",
      "^@table(.*)$": "<rootDir>/src/table/$1"
    },
    "moduleDirectories": [
      "src",
      "node_modules"
    ]
  },
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "styled-components": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.8.0",
    "clean-webpack-plugin": "^0.1.19",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-dom": "^2.1.0",
    "react-testing-library": "^5.2.0",
    "regenerator-runtime": "^0.12.1",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  }
}
